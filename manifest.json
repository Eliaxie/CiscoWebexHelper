{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "images/logo16.png",
    "32": "images/logo32.png",
    "48": "images/logo48.png",
    "128": "images/logo128.png"
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/logo16.png",
      "32": "images/logo32.png",
      "48": "images/logo48.png",
      "128": "images/logo128.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "*://politecnicomilano.webex.com/*"
      ],
      "js": [
        "helper.js"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}